{
  "functions": {
    "api/analyze.py": {
      "runtime": "@vercel/python"
    },
    "api/test.py": {
      "runtime": "@vercel/python"  
    },
    "api/health.py": {
      "runtime": "@vercel/python"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/frontend/index.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/frontend/$1"
    }
  ]
}